{
  "expo": {
    "name": "infood",
    "slug": "infood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/InFood.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/InFood.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asantesylvesterkwame.infoodapp"
    },
    "web": {
      "favicon": "./assets/InFood.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b56a9afc-d05d-43ac-b846-8eebd9ee1cc3"
      }
    }
  }
}
